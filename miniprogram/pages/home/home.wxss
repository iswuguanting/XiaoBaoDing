.bosings{
    width: 100vw;
    height: 100vh;
    /* background-color: aqua; */
}
.main{
    width: 100%;
    height: 90%;
    background-color: #f6f6f6;
    overflow-y: scroll;
    overflow-x: hidden;
    box-sizing: border-box;
    padding: 0 20rpx;
}
.conten{
    /* background-color: aqua; */
    width: 100%;
    /* height: 100%; */
}
.content-one{
    width: 100%;
    height: 250rpx;
    margin-top: 20rpx;
    padding: 40rpx 20rpx;
    box-sizing: border-box;
    border-radius: 20rpx;
    background-color: white;
    display: flex;
}
.cli{
    width: 50rpx;
    height: 100%;
    /* background-color: darkblue; */
    margin-right: 10rpx;
    position: relative;
}
.cli image{
    position: absolute;
    width: 50rpx;
    height: 50rpx;
    top: 0;
    bottom: 0;
    margin: auto 0;
}
.content-two{
    width: 170rpx;
    height: 100%;
    /* background-color: darkblue; */
}
.content-two image:nth-child(1){
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
    position: relative;
    z-index: 2;
}
.content-two image:nth-child(2){
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
    position: relative;
    z-index: 3;
}
.content-three{
    width: 400rpx;
    height:100%;
    /* background-color: darkcyan; */
    margin-left: 20rpx;
}
.content-frou{
    width: 100%;
    height: 33.3%;
    /* background-color: red; */
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: flex;
    align-items: center;
}
.content-frou:nth-child(1){
    font-weight: 600;
    font-size: 40rpx;
    
}
.content-frou:nth-child(2){
    font-size: 22rpx;
    color: rgb(172, 172, 172);
}
.content-frou:nth-child(3){
    font-size: 22rpx;
    display: flex;
   justify-content: space-between;
}

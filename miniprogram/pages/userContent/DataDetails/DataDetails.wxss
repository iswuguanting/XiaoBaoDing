.databox {
  width: 100vw;
  /* height: 100vh; */
  background-color: #eeeeee;
  padding: 20rpx;
  box-sizing: border-box;
}

.main {
  width: 100%;
  height: 100%;
  /* background-color: red; */
}

.main-top-img {
  width: 100%;
  height: 688rpx;
}

.main-top-img image {
  width: 100%;
  height: 100%;
  border-radius: 50rpx;
}

.mian-conten {
  width: 100%;
  /* height: 250rpx; */
  margin: 30rpx 0;
  background-color: white;
  border-radius: 20rpx;
  overflow: hidden;
}

.conten-mia {
  padding: 40rpx;
  font-size: 40rpx;
  font-weight: 600;
  
  display: flex;
  justify-content: space-between;
}

.cainane {
  width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  /* background-color: aquamarine; */
}

.caijia {
  color: red;
}

.mian-contento{
  width: 100%;
}

.mia-to{
  padding: 0 35rpx 30rpx;
}

.tuij{

  width: 100%;
  /* height: 400rpx; */
  background-color: white;
  border-radius: 20rpx;
  overflow: hidden;
}

.tuijian{
  width: 100%;
  height: 100rpx;
  /* background-color: #ddd; */
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 40rpx;
  font-size: 40rpx;
  font-weight: 600;
}

.the-like {
  float: left;
  width: 344rpx;
  height: 340rpx;
  /* background-color: saddlebrown; */
}

.like-img {
  width: 260rpx;
  height: 260rpx;
  /* background-color: aqua; */
  margin: 0 auto;
}

.like-img image{
  width: 100%;
  height: 100%;
  border-radius: 30rpx;
}

.like-names{
  width: 200rpx;
  height: 80rpx;
  /* background-color: salmon; */
  margin: 0 auto;
  text-align: center;
  line-height: 80rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.tuiji-conten{
  width: 100%;
  height: 100rpx;
  background-color: red;
}

.booa{
  width: 100vw;
  height: 200rpx;
}
.shoping {
  position: fixed;
  width: 100vw;
  height: 100rpx;
  bottom: 0;
  background-color: white;
  display: flex;
  justify-content: space-between;
}

.gwc {
  width: 100rpx;
  height: 100%;
  margin: 0 30rpx;
  padding: 10rpx 0;
  text-align: center;
}

.cwc-nam {
  font-size: 20rpx;
  margin-bottom: -20rpx;
  margin-top: 5rpx;
}

.gwc-img {
  width: 100%;
  height: 60%;
  position: relative;

}


.gwc-img image {
  position: absolute;
  width: 60%;
  height: 100%;
  left: 0;
  right: 0;
  margin: 0 auto;
}

.gwc-num {
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  background-color: red;
  border-radius: 999rpx;
  right: 10rpx;
  font-size: 16rpx;
  color: white;
  text-align: center;
  line-height: 30rpx;
  border: 1px solid white;
}


.goumai {
  padding: 10rpx 0;
  margin-right: 30rpx;
}

.liji {
  width: 400rpx;
  height: 100%;
  background-color: aqua;
  display: flex;
  border-radius: 999rpx;
  overflow: hidden;
}

.liji-one {
  width: 50%;
  height: 100%;
  background: linear-gradient(10deg, rgb(255, 251, 0), rgb(255, 166, 0));
  font-size: 28rpx;
  text-align: center;
  font-weight: 600;
  color: white;
  line-height: 80rpx;
}

.liji-two {
  width: 50%;
  height: 100%;
  background: linear-gradient(10deg, rgb(255, 136, 0), rgb(255, 60, 0));
  font-size: 28rpx;
  text-align: center;
  font-weight: 600;
  color: white;
  line-height: 80rpx;
}